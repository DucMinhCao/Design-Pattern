package Decorator;

interface IPizza {
	public String doPizza();
}

class TomatoPizza implements IPizza {

	@Override
	public String doPizza() {
		return "Tomato Pizza";
	}
	
}

class ChickenPizza implements IPizza {

	@Override
	public String doPizza() {
		return "Chicken Pizza";
	}
	
}

abstract class PizaaDecorator {
	
}

public class Decorator {

}
